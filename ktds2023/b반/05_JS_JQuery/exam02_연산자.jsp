<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<!-- JS 영역 -->
<script type="text/javascript">
	
	let n = 10;
	let n2 = 5;
	
	//브라우저 콘솔에 출력
	console.log(n+n2);
	console.log(n-n2);
	console.log(n*n2);
	console.log(n/n2);
	console.log(n%n2);
	
	console.log(n == n2);
	console.log(n > n2);
	console.log(n >= n2);
	console.log(n < n2);
	console.log(n <= n2);
	console.log(n != n2);
	
	
	// ==  ,  === 
	let x = 10;
	let x2 = "10";
	
	console.log(x == x2); // 값 만 비교
	console.log(x === x2); // 갑과 타입까지 비교
	
	//논리 연산자
	//3항 연산자
	
</script>
<!-- JS 영역 -->
</head>
<body>
<h2>JS 연산자</h2>
</body>
</html>