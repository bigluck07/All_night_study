<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<!--  JS 영역 -->
<script type="text/javascript">
	
	
	let n1 = 10;
	let n2 = 20;

	
	// 연산자
	console.log(n1+n2);
	console.log(n1-n2);
	console.log(n1*n2);
	console.log(n1/n2);
	console.log(n1%n2);
	console.log(" ");
	
	// 비교연산자
	console.log(n1==2);
	console.log(n1>n2);
	console.log(n1>=n2);
	console.log(n1<n2);
	console.log(n1<=n2);
	console.log(n1!=n2);
	console.log(" ");
	
	// ==, ===
	let x = 10;
	let x2 = "10";
	
	console.log(x==x2); // 값만 비교
	console.log(x===x2);// 값과 타입까지 비교
	
	//논리연산자
	//3항연산자


</script>
<!--  JS 영역 -->
</head>
<body>
<h2>JS 변수</h2>
</body>
</html>